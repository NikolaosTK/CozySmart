





@{
    ViewBag.Title = "MyBookings";
}

<h2>MyBookings</h2>

@model IEnumerable<CozySmart.Models.Booking>

@{
    ViewBag.Title = "MyBookings";
}


@if (ViewBag.Num == 0)
{



    @Html.Partial("NavbarTenant")

}

else if (ViewBag.Num == 1)
{

    @Html.Partial("NavbarTenantRole")

}




<h2>MyBookings</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Arrival)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Departure)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accommodation.Title)
        </th>

        <th>
              Address
        </th>


        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Arrival)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Departure)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accommodation.Title)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Accommodation.Adress)
        </td>

    </tr>
    }

</table>
