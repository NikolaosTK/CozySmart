





@{
    ViewBag.Title = "MyBookings";
}

<h2>MyBookings</h2>

@model IEnumerable<CozySmart.Models.Booking>

@{
    ViewBag.Title = "MyBookings";
}


@if (ViewBag.Num == 0)
{



    @Html.Partial("NavbarTenant")

}

else if (ViewBag.Num == 1)
{

    @Html.Partial("NavbarTenantRole")

}

else if (ViewBag.Num == 2)
{

    @Html.Partial("NavbarHostRole")
}



<h2>MyBookings</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Arrival)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Departure)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accommodation.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationUserId)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Arrival)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Departure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Accommodation.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationUserId)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
